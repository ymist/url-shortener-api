{
	"name": "url-shortener-api",
	"version": "1.0.0",
	"description": "This project is url shortener.",
	"main": "app.ts",
	"type": "module",
	"imports": {
		"#src/*": "./src/*"
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"start": "npm run build && fastify start -l info dist/app.js",
		"build": "tsc",
		"watch:ts": "tsc -w",
		"dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
		"dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^6.0.0",
		"@fastify/sensible": "^6.0.0",
		"@prisma/client": "^6.19.2",
		"dotenv": "^17.2.3",
		"fastify": "^5.0.0",
		"fastify-cli": "^7.4.1",
		"fastify-plugin": "^5.0.0",
		"hashids": "^2.3.0",
		"ioredis": "^5.9.2",
		"pg": "^8.17.1",
		"prisma": "^6.19.2",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/ioredis": "^4.28.10",
		"@types/node": "^24.0.8",
		"@vitest/coverage-v8": "^4.0.17",
		"@vitest/ui": "^4.0.17",
		"c8": "^10.1.2",
		"concurrently": "^9.0.0",
		"eslint": "^9.39.2",
		"fastify-tsconfig": "^3.0.0",
		"globals": "^17.0.0",
		"jiti": "^2.6.1",
		"ts-node": "^1.7.1",
		"typescript": "~5.9.2",
		"typescript-eslint": "^8.53.0",
		"vite-tsconfig-paths": "^6.0.4",
		"vitest": "^4.0.17"
	}
}
